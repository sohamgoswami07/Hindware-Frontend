{
    "allow_anonymous_view": true,
    "attributes": [
        {
            "indexed": true,
            "name": "category_name"
        },
        {
            "indexed": true,
            "name": "sub_category_name"
        },
        {
            "auto_updater": true,
            "default": {
                "args": [
                    "category_name",
                    "sub_category_name"
                ],
                "function": "name_to_id",
                "kwargs": {
                    "attribute_join": "_",
                    "case": "lower",
                    "compressor": "to_ascii_lower",
                    "is_unique": true,
                    "separator": "_",
                    "skip_empty": true,
                    "translate": [
                        "+?()&",
                        "_____"
                    ]
                }
            },
            "id": true,
            "name": "category_id",
            "type": "uid"
        },
        {
            "name": "sequence",
            "type": "number"
        },
        {
            "name": "application_image",
            "type": "image"
        },
        {
            "auto_updater": true,
            "default": {
                "args": [
                    "product"
                ],
                "function": "search_among_children",
                "kwargs": {
                    "_sort_by": "sequence"
                }
            },
            "name": "products",
            "type": "object_list"
        }
    ],
    "expand_attributes": false,
    "name": "product_category",
    "quick_views": {
        "default": [
            "category_name",
            "sub_category_name",
            "application_image"
        ],
        "download": [
            "category_id",
            "category_name",
            "sub_category_name",
            "application_image"
        ]
    },
    "timezone": "Asia/Kolkata"
}
